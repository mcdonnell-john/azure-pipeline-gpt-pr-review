  "tags": [
    "OpenAI",
    "GPT",
    "pullRequest",
    "review"
  ],
  "files": [
    {
      "path": "GPTPullRequestReview"
    }
  ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/mcdonnell-john/azure-pipeline-gpt-pr-review"
  },
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "contributions": [
    {
      "id": "OpenAIPRReviewer",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
