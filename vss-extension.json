{
    "manifestVersion": 1,
    "id": "OpenAIPRReviewer",
    "name": "OpenAI Pull Request Reviewer",
    "version": "1.0.17",
    "publisher": "mcdonnell-john",
    "galleryFlags": [
        "Preview"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "A task that review Pull Request using OpenAI",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/icon.png"
    },
    "tags": [
        "OpenAI",
        "GPT",
        "pullRequest",
        "review"
    ],
    "files": [
        {
            "path": "GPTPullRequestReview"
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/mcdonnell-john/azure-pipeline-gpt-pr-review"
    },
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "contributions": [
        {
            "id": "OpenAIPRReviewer",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "GPTPullRequestReview"
            }
        }
    ]
}